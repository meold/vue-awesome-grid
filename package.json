{
  "name": "vue-awesome-grid",
  "version": "1.0.1",
  "description": "Vue 2.0 datatables alternative to jQuery datatables",
  "main": "dist/vue-awesome-grid.umd.js",
  "module": "dist/vue-awesome-grid.esm.js",
  "unpkg": "dist/vue-awesome-grid.min.js",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/*",
    "src/*",
    "!index.hml"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && npm run copyToExample",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "copyToExample": "yes | cp  dist/vue-awesome-grid.umd.js examples/vue-awesome-grid.umd.js",
    "linter": "node_modules/.bin/eslint ./src --ext vue,js"
  },
  "devDependencies": {
    "acorn": "^8.0.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "jquery": "^3.5.1",
    "minimist": "^1.2.5",
    "rollup": "^1.29.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.5",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "node-sass": "^4.14.1"
  }
}
